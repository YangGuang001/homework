<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="resourceBean" class="com.yang.spring.test.ResourceBean" init-method="init" destroy-method="destroy">
        <property name="file" value="d:/test.txt"/>
    </bean>
    <bean id="dependentBean" class="com.yang.spring.test.DependentBean" init-method="init" destroy-method="destroy">
            <property name="resourceBean" ref="resourceBean"/>
        </bean>

</beans>